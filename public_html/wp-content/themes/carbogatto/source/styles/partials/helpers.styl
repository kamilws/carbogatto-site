//Вспомогательные стили
.clear
  clear both
for num in 3 5 7 10 15 20 30 40 50 60 70 80 90 100 110 120 130
  .height-{num}
    height (num) px
    padding 0
    margin 0
.delim-5
  display inline-block
  width 5px
  height 2px
.relative
  position relative
.clearfix
  &:before
  &:after
    content " "
    display table
  &:after
    clear both
.float-left
  float left
.float-right
  float right
.text-right
  text-align right
.text-left
  text-align left
.text-center
  text-align center
.font-bold
  font-weight bold
.font-normal
  font-weight normal
.hide
  display none
.show
  display block
.display-block
  display block
.display-inline
  display inline
.display-inline-block
  display inline-block
.pre-line
  white-space pre-line
.transparent
  opacity .01
for num in 5 10 15 20
  .delim-{num}
    width  (num)px
    height 2px
    display inline-block
.break-word
  word-wrap break-word